# Public poll start date shown in the UI. Parsed with `new Date()`.
# Use ISO 8601 (recommended) or a numeric timestamp string.
# Example: 2025-12-04T00:00:00Z
NEXT_PUBLIC_POLL_DATE=

# Public URL to the salary CSV or sheet export. Must be publicly accessible
# if fetched from the client. Example: https://.../export?format=csv
NEXT_PUBLIC_SALARY_SHEET_URL=

# Public URL to the category CSV or sheet export used by the salary API.
# Example: https://.../export?format=csv
NEXT_PUBLIC_CATEGORY_SHEET_URL=

# Server-only secret used to authenticate internal endpoints (e.g. revalidate).
# Keep this private and do NOT commit real values. Example: a long random string
SECRET=

# Server-only API token sent as `auth-client` header for external requests.
# Keep this private (do NOT expose as NEXT_PUBLIC_). Example: sk_live_xxx
DOLARITO_TOKEN=
